<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Entities\Message" table="message" repository-class="Entities\MessageRepository">
    <id name="id" type="bigint" column="id">
      <generator strategy="AUTO"/>
    </id>
    <field name="date" type="bigint" column="date"/>
    <field name="number" type="text" column="senderMsisdn" nullable="true" />
    <field name="text" type="text" column="textContent" nullable="true" />
    <field name="deleted" text="integer" column="gouwradioDeleted" />
    <field name="type" text="integer" column="gouwradioType" nullable="true" />
    <field name="new" text="integer" column="gouwradioNew" />
  </entity>
</doctrine-mapping>
